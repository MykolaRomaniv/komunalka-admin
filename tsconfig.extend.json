{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "components": ["app/components"],
      "containers": ["app/containers"],
      "services": ["app/services"],
      "constants": ["app/constants"],
      "common": ["app/common"],
      "hooks": ["app/hooks"],
      "types": ["app/types"],

      "components/*": ["app/components/*"],
      "containers/*": ["app/containers/*"],
      "services/*": ["app/services/*"],
      "constants/*": ["app/constants/*"],
      "common/*": ["app/common/*"],
      "hooks/*": ["app/hooks/*"],
      "types/*": ["app/types/*"]
    }
  }
}
